{
    "name": "adreece/laracord-live-chat",
    "description": "A Laravel package for live chat with Discord integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AD Reece",
            "email": "reece@ascend-digital.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "ADReece\\LaracordLiveChat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ADReece\\LaracordLiveChat\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ADReece\\LaracordLiveChat\\LaracordLiveChatServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
